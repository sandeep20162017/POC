function getSupplierNameParams() {
    // Get currently edited row's model (in the popup editor)
    var grid = $("#nscPartsGrid").data("kendoGrid");
    var model = grid.editable ? grid.editable.options.model : null;

    return {
        supplierNumber: model ? model.OrigSuppNum : ""  // Pass OrigSuppNum if available
    };
}
