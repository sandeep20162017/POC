INSERT INTO [SBCES].[ArchVehScPartsUsed] (
    [MmsStockCode],
    [DateEntered],
    [RebPartCost],
    [UserEntered],
    [MmsCost],
    [OemCost],
    [MmsRebuiltCode],
    [CostCentre],
    [QtyReqd],
    [PercentUsage],
    [LinkCode],
    [RebuiltPart],
    [LinkType],
    [Corecost],
    [ArchVehicleListId],
    [TotalCost],
    [ScKeyword],
    [ScDescription]
)
SELECT
    spu.[MmsStockCode],
    spu.[DateEntered],
    spu.[RebPartCost],
    spu.[EnteredBy] AS [UserEntered],
    spu.[MmsCost],
    spu.[OemCost],
    spu.[MmsRebuiltCode],
    spu.[CostCentre],
    spu.[QtyReqd],
    spu.[PercentUsage],
    spu.[LinkCode],
    spu.[RebuiltPart],
    spu.[LinkType],
    spu.[CoreCost],
    -- Get MAX archive ID for the vehicle --
    (SELECT MAX(avl.[ArchVehicleListId])
     FROM [SBCES].[ArchVehicleList] avl 
     WHERE avl.OldId = TRY_CAST(LTRIM(RTRIM(spu.[LinkCode])) AS INT),
    NULL AS [TotalCost],
    NULL AS [ScKeyword],
    NULL AS [ScDescription]
FROM [SBCES].[ScPartsUsed] spu
WHERE
    TRY_CAST(LTRIM(RTRIM(spu.[LinkCode])) AS INT) IS NOT NULL
    -- Ensure vehicle exists in archive --
    AND EXISTS (
        SELECT 1 
        FROM [SBCES].[ArchVehicleList] avl 
        WHERE avl.OldId = TRY_CAST(LTRIM(RTRIM(spu.[LinkCode])) AS INT
    );
