CES_ARCHIVE.GET_ARCH_VEH_RB_PARTS_USED
CES_ARCHIVE.GET_ARCH_VEH_SC_PARTS_USED
CES_ARCHIVE.GET_ARCH_VEH_NSC_PARTS_USED
--------------------------------------
CES_ARCHIVE.GET_ARCH_VEH_EMPLOYEE_LABOUR
CES_ARCHIVE.GET_ARCH_VEH_LABOUR_CC_TOTAL
CES_ARCHIVE.GET_ARCH_VEH_MASTERLIST_BY_ID 
----------------------------------------
SELECT
            *
        FROM
            ARCH_VEH_SC_PARTS_USED
        WHERE
            arch_vehicle_list_id = vi_arch_veh_masterlist_id and
            rebuilt_part = 'N';

 SELECT
            *
        FROM
            ARCH_VEH_NSC_PARTS_USED
        WHERE
            arch_vehicle_list_id = vi_arch_veh_masterlist_id;

 SELECT
            *
        FROM
            ARCH_VEH_SC_PARTS_USED
        WHERE
            arch_vehicle_list_id = vi_arch_veh_masterlist_id and
            rebuilt_part = 'Y';

 SELECT
             labour_defn AS "Labour Type",
             costcentre AS "CC",
             task AS "Task",
             usage AS "Usage",
             hrs_reqd AS "Time",
             adj_hrs AS "Actual Time",
             time_addition as "Time Addition"
        FROM
            ARCH_VEH_EMPLOYEE_LABOUR
        WHERE
            arch_vehicle_list_id = vi_arch_veh_masterlist_id;

 SELECT

            cost_centre AS "Cost Centre",

            DECIMAL_MINUTES_TO_HHMM(total_hours) AS "Total Hours"
        FROM
            ARCH_VEH_LABOUR_CC_TOTAL
        WHERE
            arch_vehicle_list_id = vi_arch_veh_masterlist_id;
   SELECT
            *
        FROM
             ARCH_VEHICLE_LIST
        WHERE
            arch_vehicle_list_id = vi_arch_veh_masterlist_id;
