function cancelScPartPopup() {
    var wnd = $("#ScPartsPopupWindow").data("kendoWindow");
    if (wnd) wnd.close();

    var parentWin = $("#estimatePopup").data("kendoWindow");
    if (parentWin) parentWin.toFront();
}


and

$("#ScPartsPopupWindow").kendoWindow({
    width: "500px",
    height: "600px",
    modal: false, // prevent double overlay conflict
    visible: false,
    resizable: false,
    draggable: true,
    title: "Stock Coded Part",
    actions: ["Close"]
});
