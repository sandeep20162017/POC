function cancelScPartPopup() {
    var scWin = $("#ScPartsPopupWindow").data("kendoWindow");
    if (scWin) scWin.close();

    var parentWin = $("#estimatePopup").data("kendoWindow");
    if (parentWin) {
        // âœ… reapply modal overlay for parent
        $(".k-overlay").remove(); // remove old overlay
        $("<div class='k-overlay' style='z-index:9999'></div>").insertBefore(parentWin.wrapper);
        parentWin.wrapper.css("z-index", 10000);
        parentWin.toFront();
    }
}
