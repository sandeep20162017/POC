Dropdown field values are displayed as undefined, undefined.
Pls help fix
 <div class="form-group  double-width">
    
         @Html.LabelFor(m => m.MbNumber, "Make Vs Buy Prefix", new { @class = "form-label" })
         @(Html.Kendo().DropDownList()
         .Name("PrefixCode")
         .DataTextField("PrefixCode")
         .DataValueField("PrefixCode")
            
             .AutoBind(true)
             .Value(Model.MbNumber)
             .DataSource(ds =>
             {
             ds.Read(r => r.Action("ReadMbPrefix", "MakeVsBuy"));
             })
             .Enable(isNew)   //  NEW RECORD: enabled; EDIT: disabled
             )

        [HttpGet]
        [AllowAnonymous]
        public async Task<IActionResult> ReadMbPrefix([DataSourceRequest] DataSourceRequest request)
        {
            var mbPrefix = await GetMbPrefix();
            return Json(mbPrefix);
        }

        [AllowAnonymous]
        private async Task<IEnumerable<MbPrefixModel>> GetMbPrefix() //multiselect
        {
            try
            {
                var query = @"
                         SELECT distinct [PrefixCode] 
                          FROM [CES].[SBCES].[MBPrefixSequence]";
                var mbPrefix = await _dbConnection.QueryAsync<MbPrefixModel>(query);
                return mbPrefix.ToList();
            }
            catch (Exception ex)
            {
                return null;
            }
        }
