Error : The data types varchar and varchar are incompatible in the subtract operator.
SBCES.FinalCost(
                CAST(spu.PercentUsage AS DECIMAL(20,10)), 
                CAST(spu.QtyReqd AS DECIMAL(20,10)), 
                CAST(spu.CoreCost AS DECIMAL(20,10)), 
                0)
            ) * COALESCE(pm.TotalMultiplier, 1) AS AdjustedCost,
            SBCES.FinalCost(
                CAST(spu.PercentUsage AS DECIMAL(20,10)), 
                CAST(spu.QtyReqd AS DECIMAL(20,10)), 
                CAST(spu.MmsCost AS DECIMAL(20,10)), 
                0) * COALESCE(pm.TotalMultiplier, 1) AS TaxableCost

ALTER FUNCTION [SBCES].[FinalCost]
    (@ViPercent VARCHAR(20),
     @ViQuantity VARCHAR(20),
     @ViCost DECIMAL(10,2),
     @ViCoreCost DECIMAL(10,2))
RETURNS VARCHAR(4000) AS
BEGIN
    DECLARE @Total DECIMAL(38,4);
 
    SET @Total = (CAST(REPLACE(ISNULL(@ViPercent,'0'),',','.') AS DECIMAL(10,4))/ 100) * CAST(@ViQuantity AS DECIMAL(10,4)) * @ViCost;
    RETURN ROUND(@Total - @ViCoreCost, 2);
END;

