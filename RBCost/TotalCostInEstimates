using System.ComponentModel.DataAnnotations;

namespace BCES.Models.Parts
{
   
        public class TotalCostInEstimates
    {
           //public decimal CoreCost
            public string RebuiltPartNum { get; set; }

            public decimal CoreCost { get; set; }
